{
  "[javascriptreact]": {},
  "color-highlight.markerType": "underline",
  "csharp.suppressDotnetInstallWarning": true,
  "editor.fontFamily": "Operator Mono, Hack, Menlo, Monaco, monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.formatOnPaste": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderIndentGuides": true,
  "editor.rulers": [
    120
  ],
  "editor.selectionHighlight": false,
  "editor.snippetSuggestions": "top",
  "editor.tabSize": 2,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 120,
  "files.associations": {
    "*.css": "source.css.styled",
    "*.js": "javascriptreact",
    "*.jsx": "javascriptreact"
  },
  "git.confirmSync": false,
  "java.errors.incompleteClasspath.severity": "ignore",
  "javascript.validate.enable": false,
  "jest.enableCodeLens": false,
  "search.exclude": {
    "**/bower_components": true,
    "**/node_modules": false
  },
  "sublimeTextKeymap.promptV3Features": true,
  "window.nativeTabs": false,
  "window.zoomLevel": 0,
  "workbench.colorTheme": "Tomorrow Night Operator Mono",
  "workbench.iconTheme": "file-icons",
  "workbench.startupEditor": "newUntitledFile",
  "jest.autoEnable": false,
  "jest.enableSnapshotUpdateMessages": false,
  "jest.enableSnapshotPreviews": false
}